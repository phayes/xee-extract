<?xml version="1.0" encoding="UTF-8"?>
<t:transform xmlns:t="http://www.w3.org/1999/XSL/Transform" version="3.0">
   <!-- Purpose: xsl:message outputs a comment -->
   <t:template match="/">
      <out>
         <t:message>
           <t:comment>Chicken legs</t:comment>
         </t:message>
      </out>
   </t:template>
</t:transform>
