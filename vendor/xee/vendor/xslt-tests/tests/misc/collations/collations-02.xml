<?xml version="1.0"?>
<!-- Test for xsl:sort -->
<doc>
  <!-- simple -->
  <item target="ab"  num="1" />
  <item target="cb"  num="2" />
  <item target="dc"  num="3" />
  <item target="cd"  num="4" />
  <item target="ac"  num="5" />
  
  <!-- secondary difference Accents -->
  <item target="ab"  num="6" />
  <item target="äb"  num="7" />
  <item target="zb"  num="8" />
  <item target="a"  num="9" />
  <item target="ä"  num="10" />
  
  <!-- tertiary difference - case-->
  <item target="AÄ"  num="11" />
  <item target="AZ"  num="12" />
  <item target="aÄ"  num="13" />
  <item target="Az"  num="14" />
  
  <!-- Quaternary difference - punctuation-->
  <item target="a-b"  num="15" />
  <item target="b-c"  num="16" />
  <item target="b  c- "  num="17" />
  <item target="--c--"  num="18" />
  <item target="ab!c!d!"  num="19" />
  <item target="-a-bcd"  num="20" />
  <item target="cd!"  num="21" />
  <item target="  b c "  num="22" />
  
</doc>