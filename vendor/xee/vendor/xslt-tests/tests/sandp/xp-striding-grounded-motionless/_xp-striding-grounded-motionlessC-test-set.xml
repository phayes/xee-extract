<?xml version="1.0" encoding="UTF-8"?>
<test-set xmlns="http://www.w3.org/2012/10/xslt-test-catalog" name="xp-striding-grounded-motionlessC">
   <description>
                    CP: Striding, RP: Grounded, RS: Motionless
                    Self-axis that changes axes selects nothing, so grounded/motionless
                </description>
   <dependencies>
      <sweep_and_posture satisfied="true" value="supports-sweep-and-posture-assessments"/>
   </dependencies>
   <test-case name="xp-striding-grounded-motionlessC001">
      <description>
                CP: striding, RP: grounded, RS: motionless
                Base expression: self::foo
                Separators:      / 
                With axes:       self
                With node types: any
                Invariant:       node-type-unequal
            </description>
      <created by="Abel Braaksma" on="2014-10-16"/>
      <modified by="Abel Braaksma" on="2014-10-21" change="Added dependency for sweep and posture tests, added @part attribute for subtests"/>
      <modified by="Abel Braaksma" on="2014-10-28" change="Changed node-type for namespace-node() tests w/o axis specifier from error to namespace, see bug#26788 (only applies to relevant tests)"/>
      <test>
         <posture-and-sweep context-posture="striding">
            <xpath part="1">self::foo/self::document-node()</xpath>
            <xpath part="2">self::foo/self::attribute()</xpath>
            <xpath part="3">self::foo/self::attribute(*)</xpath>
            <xpath part="4">self::foo/self::attribute(ex:foo)</xpath>
            <xpath part="5">self::foo/self::attribute(Q{http://example.com}foo)</xpath>
            <xpath part="6">self::foo/self::comment()</xpath>
            <xpath part="7">self::foo/self::processing-instruction()</xpath>
            <xpath part="8">self::foo/self::processing-instruction(xml-stylesheet)</xpath>
            <xpath part="9">self::foo/self::processing-instruction('xml-stylesheet')</xpath>
            <xpath part="10">self::foo/self::text()</xpath>
            <xpath part="11">self::foo/self::namespace-node()</xpath>
         </posture-and-sweep>
      </test>
      <result>
         <assert-posture-and-sweep result-posture="grounded" result-sweep="motionless"/>
      </result>
   </test-case>
