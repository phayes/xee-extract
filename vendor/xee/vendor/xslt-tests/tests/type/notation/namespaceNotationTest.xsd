<?xml version="1.0" encoding="UTF-8" ?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"	xmlns:smokey="http://notation.example.com" targetNamespace="http://notation.example.com"	xmlns:test="http://test.notation.example.com"	xmlns:diff="http://different.notation.example.com" elementFormDefault="qualified">		<xsd:import namespace="http://different.notation.example.com" schemaLocation="importSchema.xsd"/>	<xsd:import namespace="http://test.notation.example.com" schemaLocation="simpleNamespaceNotation.xsd"/>		<xsd:annotation>		<xsd:documentation>      		Schema for xs:NOTATION 		</xsd:documentation>	</xsd:annotation><!-- Declarations -->	<xsd:notation name="wav" public="audio/wav" system="mplay32.exe"/>	<xsd:notation name="mp3" public="audio/mpeg" system="mplay32.exe"/>	<xsd:notation name="rmi" public="audio/mid" system="mplay32.exe"/>	<xsd:notation name="rar" public="audio/mid" system="mplay.exe"/>	<xsd:notation name="jpeg" public="image" system="viewer.exe"/>	<xsd:notation name="exe" public="executable" system="cmd"/>	<!-- Begin definitions -->		<xsd:element name="NOTATION-enumeration" type="smokey:not0-attribute" />	<!-- Use an intermediary so that type can be applied to attributes only -->	<xsd:complexType name="not0-attribute">		<xsd:sequence>			<xsd:element name="NOTATION-element" type="smokey:not1-attribute" minOccurs="1" maxOccurs="unbounded"/>			<xsd:element ref="test:doc" minOccurs="0" maxOccurs="1"/> 		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="not1-attribute">		<xsd:sequence>			<xsd:element name="NOTATION-subelement" type="xsd:string" minOccurs="1"/> 		</xsd:sequence>		<xsd:attribute name="NOTATION-attribute" type="smokey:not1-NOTATION-enumeration-Type" use="required"/>		<xsd:attribute name="name" type="xsd:string" use="required"/>	</xsd:complexType>	<!-- Following are types derived from NOTATION -->	<xsd:simpleType name="not1-NOTATION-enumeration-Type">		<xsd:restriction base="xsd:NOTATION">			<xsd:enumeration value="smokey:mp3" />			<xsd:enumeration value="smokey:rmi" />			<xsd:enumeration value="smokey:wav" />			<xsd:enumeration value="smokey:rar" />			<xsd:enumeration value="smokey:jpeg" />			<xsd:enumeration value="diff:mp3" />		</xsd:restriction>	</xsd:simpleType>		<!-- Following are types derived from not1-NOTATION-enumeration-Type -->	<xsd:simpleType name="not1-NOTATION-derived-Type">		<xsd:restriction base="smokey:not1-NOTATION-enumeration-Type">			<xsd:enumeration value="smokey:mp3" />			<xsd:enumeration value="smokey:wav" />			<xsd:enumeration value="diff:mp3" />		</xsd:restriction>	</xsd:simpleType></xsd:schema>