         <schema role="source-reference" file="variousTypesSchemaFunc.xsd" xsd-version="1.0"/>
      </environment>
      <dependencies>
         <spec value="XSLT20+"/>
         <year_component_values value="support negative year"/>
      </dependencies>
      <test>
