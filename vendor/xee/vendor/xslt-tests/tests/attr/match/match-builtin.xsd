	<!-- Following are types derived from decimal -->

	<xsd:simpleType name="de1-decimal-enumeration-Match">
		<xsd:restriction base="xsd:decimal">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="-1"/>
			<xsd:enumeration value="1.0001"/>
			<xsd:enumeration value="-1.0001"/>
			<xsd:enumeration value="1.001"/>
			<xsd:enumeration value="-1.001"/>			
			<xsd:enumeration value="1.1"/>
			<xsd:enumeration value="-1.1"/>
			<xsd:enumeration value="1.01"/>
			<xsd:enumeration value="-1.01"/>			
			<xsd:enumeration value="1.0101"/>
			<xsd:enumeration value="-1.0101"/>
			<xsd:enumeration value="11111"/>
			<xsd:enumeration value="-11111"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="de2-decimal-maxExclusive-Match">
		<xsd:restriction base="xsd:decimal">
			<xsd:maxExclusive value="123.456789"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="de3-decimal-maxInclusive-minExclusive-Match">
		<xsd:restriction base="xsd:decimal">
			<xsd:minExclusive value="-1"/>
			<xsd:maxInclusive value="1048.576"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="de4-decimal-minInclusive-pattern-Match">
		<xsd:restriction base="xsd:decimal">
			<xsd:pattern value="\d{4}\.\d{3}"/>
			<xsd:minInclusive value="1000.000"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="de5-decimal-fractionDigits-totalDigits-Match">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="11"/>
			<xsd:fractionDigits value="4"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="de6-decimal-whiteSpace-Match">
		<xsd:restriction base="xsd:decimal">
			<xsd:whiteSpace value="collapse"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
