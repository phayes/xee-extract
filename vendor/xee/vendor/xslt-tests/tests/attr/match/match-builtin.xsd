			<xsd:element ref="elem-NCName-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-NMTOKEN-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-int-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-nonPositiveInteger-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-positiveInteger-match" minOccurs="0" maxOccurs="1"/>
