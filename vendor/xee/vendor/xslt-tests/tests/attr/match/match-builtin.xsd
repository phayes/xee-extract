					<xsd:attribute ref="attr-positiveInteger-match" use="optional"/>
					<xsd:attribute ref="attr-nonNegativeInteger-match" use="optional"/>
					<xsd:attribute ref="attr-negativeInteger-match" use="optional"/>
					<xsd:attribute ref="attr-long-match" use="optional"/>
					<xsd:attribute ref="attr-unsignedLong-match" use="optional"/>
