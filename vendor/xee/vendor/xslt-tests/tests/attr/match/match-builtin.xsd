			<xsd:element ref="elem-decimal-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-double-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-duration-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-float-match" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="elem-gDay-match" minOccurs="0" maxOccurs="1"/>
