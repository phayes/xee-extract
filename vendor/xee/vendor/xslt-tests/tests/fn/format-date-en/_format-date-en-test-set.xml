   <test-case name="format-date-en-029">
      <description>XSLT 2.0: test format-date: English BC/AD</description>
      <created by="Michael Kay" on="2012-11-07"/>
      <dependencies>
         <spec value="XSLT20+"/>
         <year_component_values value="support negative year"/>
         <default_calendar_in_date_formatting_functions value="AD"/>
      </dependencies>
      <test>
         <stylesheet file="format-date-en-029.xsl"/>
         <initial-template name="main"/>
      </test>
      <result>
         <assert-xml><![CDATA[<out>;
<z>1990AD</z>;
<z>55BC</z>;   
</out>]]></assert-xml>
